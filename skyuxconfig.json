{
  "$schema": "./node_modules/@skyux/config/skyuxconfig-schema.json",
  "mode": "easy",
  "compileMode": "aot",
  "auth": false,
  "name": "ga-example",
  "app": {
    "port": "9704",
    "styles": [
      "@skyux/theme/css/sky.css",
      "src/app/styles.scss"
    ],
    "theming": {
      "supportedThemes": ["default", "modern"],
      "theme": "default"
    }
  },
  "host": {
    "bbCheckout": {
      "version": "2"
    }
  },
  "appSettings": {
    "localhost": "http://localhost"
  },
  "params": {
  },
  "codeCoverageThreshold": "standard",
  "pipelineSettings": {
    "checkMarx": {
      "enabled": true
    }
  }
}
